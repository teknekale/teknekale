{
  "name": "teknekale",
  "description": "Personal Blog",
  "version": "1.0.0",
  "author": "Simone Pachera",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "lint": "xo",
    "cov": "codecov --token=ff7974b4-1cab-4197-b1d4-38630da90685",
    "test": "jest",
    "codecheck": "xo && jest && yarn cov"
  },
  "dependencies": {
    "gatsby": "^1.6.5",
    "gatsby-link": "^1.6.5",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "gatsby-plugin-typography": "^1.7.3",
    "lodash": "^4.17.4",
    "semantic-ui-react": "0.71.4",
    "typography-theme-bootstrap": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.2.0",
    "jest": "^20.0.4",
    "xo": "^0.18.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "xo": {
    "space": true,
    "extends": [
      "xo-react"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "import/no-unassigned-import": [
        "off"
      ],
      "import/no-unresolved": [
        "off"
      ],
      "react/forbid-component-props": [
        "off"
      ],
      "indent": [
        "warn",
        2
      ]
    }
  },
  "keywords": [
    "teknekale",
    "simone pachera",
    "pachera"
  ]
}
